{
  "version": 2,
  "builds": [
    {
      "src": "fakenews_project/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "fakenews_project/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "fakenews_project.settings"
  },
  "functions": {
    "fakenews_project/wsgi.py": {
      "maxDuration": 30
    }
  }
}
